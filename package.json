{
  "name": "cypress-js",
  "version": "1.0.0",
  "description": "CypressIO based test automation for GhostCMS",
  "main": "index.js",
  "scripts": {
    "cms:install": "npm install ghost-cli@latest -g && ghost install local --dir ghost-cms",
    "cms:start": "ghost start --dir ghost-cms",
    "cms:stop": "ghost stop --dir ghost-cms",
    "cms:uninstall": "npm run cms:stop && npm uninstall ghost-cli@latest -g && rm -rf ghost-cms",
    "pretest": "npm run cms:start",
    "test": "$(npm bin)/cypress run",
    "test:chrome": "$(npm bin)/cypress run --browser chrome",
    "test:firefox": "$(npm bin)/cypress run --browser firefox",
    "test:edge": "$(npm bin)/cypress run --browser edge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QualityAssuranceTeam/cypress-js.git"
  },
  "author": "yahmedov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QualityAssuranceTeam/cypress-js/issues"
  },
  "homepage": "https://github.com/QualityAssuranceTeam/cypress-js#readme",
  "devDependencies": {
    "cypress": "^4.0.2"
  }
}
