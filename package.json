{
  "name": "cypress-js",
  "version": "1.0.0",
  "description": "CypressIO based test automation for Ghost CMS",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm uninstall ghost-cli@latest -g && rm -rf ghost-cms",
    "postinstall": "npm install ghost-cli@latest -g && ghost install local --dir ghost-cms",
    "cms:start": "ghost start --dir ghost-cms",
    "cms:stop": "ghost stop --dir ghost-cms",
    "pretest": "npm run cms:start",
    "test": "$(npm bin)/cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QualityAssuranceTeam/cypress-js.git"
  },
  "author": "yahmedov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QualityAssuranceTeam/cypress-js/issues"
  },
  "homepage": "https://github.com/QualityAssuranceTeam/cypress-js#readme",
  "devDependencies": {
    "cypress": "^3.8.3"
  }
}
